/* 多个左滚动 - 公用类 */ 
.leftLoop{ overflow:hidden; position:relative;  border:1px solid #ccc;   } 
.leftLoop .hd{ overflow:hidden;  height:30px; background:#f4f4f4;  } 
.leftLoop .prev,.leftLoop .next{ display:block;  width:26px; height:63px; line-height:10px; float:right;  overflow:hidden;   
     cursor:pointer; background:url("../images/link_right.png") no-repeat;position:absolute;z-index:99;right:0px;top:50%;margin-top:-32px;} 
.leftLoop .next{ background:url("../images/link_left.png") no-repeat;  left:0px;right:auto;} 
.leftLoop .hd ul{ float:right; overflow:hidden; zoom:1; margin-top:10px;  } 
.leftLoop .hd ul li{ position:relative; _display:inline; float:left;  width:10px; height:10px; overflow:hidden; background:#ccc; margin-right:10px; text-indent:-999px; cursor:pointer; 
    -webkit-border-radius: 10px; -moz-border-radius: 10px; border-radius: 10px; } 
.leftLoop .hd ul li.on{ background:#1A71C3;  } 
.leftLoop .bd{ padding:10px 40px;   } 
.leftLoop .bd ul{ overflow:hidden; zoom:1; } 
.leftLoop .bd ul li{ margin:0 10px; text-align:center; float:left; display:inline; } 
